.SUFFIXES: .cpp

srcdir		= @srcdir@

COMPILE		= @CXX@ @DEFS@ $(VJ_INCLUDES) @CPPFLAGS@ @CXXFLAGS@
LINK		= @CXX@ @CPPFLAGS@ @CXXFLAGS@ @EXTRA_LINK_FLAGS@
VJ_INCLUDES	= @INCLUDES@ -I$(VJ_BASE_DIR)/include -I$(srcdir)
VPATH		= @srcdir@

OBJECTS		= main.o

# -----------------------------------------------------------------------------
# Targets.
# -----------------------------------------------------------------------------
default: all

all: run

run: $(OBJECTS)
	$(LINK) -o $@ $(OBJECTS)

.cpp.o:
	$(COMPILE) @OBJ_NAME_FLAG@ -c $<

# -----------------------------------------------------------------------------
# Clean-up.
# -----------------------------------------------------------------------------
clean:
	rm -f Makedepend run *.exe *.ilk $(OBJECTS) *core *.?db so_locations
	rm -rf ./ii_files
