# -----------------------------------------------------------------------------
# Makefile.in for c2lib/Kernel/testDynamicCastSTL
# $Id$
# -----------------------------------------------------------------------------

.SUFFIXES	= .o .c .C

SHELL		= /bin/sh

bindir		= @bindir@
datadir		= @datadir@
exec_prefix	= @exec_prefix@
includedir	= @includedir@
libdir		= @libdir@
libexecdir	= @libexecdir@
prefix		= @prefix@
sbindir		= @sbindir@
srcdir		= @srcdir@
sysconfdir	= @sysconfdir@
topdir		= @topdir@
top_srcdir	= @top_srcdir@
C2ROOT		= ${top_srcdir}
C2ROOT_ABS	= @C2ROOT_ABS@
C2_SRCDIR	= @C2_SRCDIR@/Kernel/testDynamicCastSTL
WORKDIR		= ${topdir}/Kernel/testDynamicCastSTL

CXX		= @CXX@
CXXCPP		= @CXXCPP@
CPPFLAGS	= @CPPFLAGS@
CXXFLAGS	= @CXXFLAGS@ ${OPTIMIZER}
DEFS		= @DEFS@
DEPENDFLAGS	= @DEPENDFLAGS@ -I${srcdir} @INCLUDES@	\
                  -I${C2ROOT_ABS} -I${topdir}
DEPEND_EXTRAS	= @DEPEND_EXTRAS@ ${DEFS}
INCLUDES	= -I${srcdir} @INCLUDES@ -I${C2ROOT} -I${topdir}
LDFLAGS		= @LDFLAGS@ -L/usr/lib${LIBBITSUF} -L/usr/lib
LIBS		= @LIBS@
OBJDIR		= .
OPTIMIZER	= -g
VPATH		= @srcdir@

CXX_COMPILE	= ${CXX} ${DEFS} ${INCLUDES} ${CPPFLAGS} ${CXXFLAGS}

C2RELEASE	= @C2RELEASE@
LIBBITSUF	= @LIBBITSUF@
PLATFORM	= @PLATFORM@

DEPEND_SRCS	= main.C

OBJECTS		= main.o

@SET_MAKE@

# -----------------------------------------------------------------------------
# Targets.
# -----------------------------------------------------------------------------

default: all

all: run

run: ${OBJECTS}
	${CXX_COMPILE} -o $@ ${OBJECTS}

main.o: main.C
	${CXX_COMPILE} -c -o $@ ${C2_SRCDIR}/main.C

.C.o:
	${CXX_COMPILE} -c -o $@ $<

depend: .depend_done

newdepend:
	rm .depend_done && ${MAKE} depend

# This target will allow the 'depend' target to be built only once.
.depend_done:
	cd ${srcdir} &&							\
          makedepend -f${WORKDIR}/Makefile -p'$${OBJDIR}/'		\
              ${DEPENDFLAGS} -- ${DEPEND_EXTRAS} -- ${DEPEND_SRCS} &&	\
          touch ${WORKDIR}/.depend_done

# -----------------------------------------------------------------------------
# Clean-up.
# -----------------------------------------------------------------------------
clean:
	rm -f .depend_done Makedepend run ${OBJECTS} core so_locations

clobber: clean

