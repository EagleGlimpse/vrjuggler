.SUFFIXES	= .o .c .C

SHELL		= /bin/sh

includedir	= @includedir@
libdir		= @libdir@
srcdir		= @srcdir@
topdir		= @topdir@
top_srcdir	= @top_srcdir@
VJROOT		= ${top_srcdir}
VJROOT_ABS	= @VJROOT_ABS@
VJ_SRCDIR	= @VJ_SRCDIR@/test/testDynamicCastSTL
WORKDIR		= ${topdir}/test/testDynamicCastSTL

# Include common definitions.
include @topdir@/Makefile.base

DEPENDFLAGS	+= -I${srcdir}
INCLUDES	+= -I${srcdir}
OPTIMIZER	= -g

DEPEND_SRCS	= main.C

OBJECTS		= main.o

@SET_MAKE@

# -----------------------------------------------------------------------------
# Targets.
# -----------------------------------------------------------------------------

default: all

all: run

run: ${OBJECTS}
	${CXX_COMPILE} -o $@ ${OBJECTS}

main.o: main.C
	${CXX_COMPILE} -c -o $@ ${VJ_SRCDIR}/main.C

# -----------------------------------------------------------------------------
# Clean-up.
# -----------------------------------------------------------------------------
clean:
	rm -f .depend_done Makedepend run ${OBJECTS} core so_locations

