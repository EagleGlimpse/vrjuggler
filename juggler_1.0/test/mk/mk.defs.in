# IRIX
CC = CC
CXX_COMPILE = CC
CXX_FLAGS = -LANG:std -n32 -mips3
C_COMPILE = cc
C_FLAGS = $(CXX_FLAGS)
EXE_LINKER = $(CXX_COMPILE)
EXE_FLAGS = -LANG:std -n32 -mips3
SO_LINKER = $(CXX_COMPILE)
SO_FLAGS = -LANG:std -shared -KPIC -all
DEFS =
WARNINGS_FLAGS = -woff 3303,1001
OBJ_FILE_SUFFIX = o

FILENAME = $(PROJECT_NAME)
LIB_FILENAME = lib$(PROJECT_NAME).so

OUTPUT_EXE_FLAG = -o $(FILENAME)
OUTPUT_OBJ_FLAG = -c -o $@
OUTPUT_LIB_FLAG = -o $(LIB_FILENAME)
