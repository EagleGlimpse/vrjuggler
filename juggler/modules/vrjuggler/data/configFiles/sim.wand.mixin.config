  <proxyAlias name="VJButton4">
    <aliasName>"VJButton4" </aliasName>
    <proxyPtr>"Button4Proxy" </proxyPtr>
  </proxyAlias>
  <proxyAlias name="VJButton5">
    <aliasName>"VJButton5" </aliasName>
    <proxyPtr>"Button5Proxy" </proxyPtr>
  </proxyAlias>
  <PosProxy name="Wand Proxy">
    <device>"SimWandPos" </device>
    <unit>"0" </unit>
    <etrans>"True" </etrans>
    <translate>"0.0" "0.0" "0.0" </translate>
    <rotate>"0.0" "0.0" "0.0" </rotate>
    <useFilter>"False" </useFilter>
    <sigmoidParams>
        <sigmoid_linear_pos_filter name="Wand Proxy-&gt;sigmoidParams-&gt;sigmoidParams 0">
          <minDist>"0.05" </minDist>
          <maxDist>"0.2" </maxDist>
          <maxThreshold>"0.0" </maxThreshold>
        </sigmoid_linear_pos_filter>
</sigmoidParams>
  </PosProxy>
  <DigProxy name="Button0Proxy">
    <device>"SimWandButtons" </device>
    <unit>"0" </unit>
  </DigProxy>
  <DigProxy name="Button2Proxy">
    <device>"SimWandButtons" </device>
    <unit>"2" </unit>
  </DigProxy>
  <DigProxy name="Button1Proxy">
    <device>"SimWandButtons" </device>
    <unit>"1" </unit>
  </DigProxy>
  <DigProxy name="Button3Proxy">
    <device>"SimWandButtons" </device>
    <unit>"3" </unit>
  </DigProxy>
  <DigProxy name="Button4Proxy">
    <device>"SimWandButtons" </device>
    <unit>"4" </unit>
  </DigProxy>
  <DigProxy name="Button5Proxy">
    <device>"SimWandButtons" </device>
    <unit>"5" </unit>
  </DigProxy>
  <Keyboard name="Wand Keyboard">
    <width>"200" </width>
    <height>"200" </height>
    <origin>"950" "350" </origin>
    <msens>"2.0" </msens>
    <display_number>"-1" </display_number>
    <lock_key>"KEY_L" </lock_key>
    <start_locked>"False" </start_locked>
    <sleep_time>"75" </sleep_time>
  </Keyboard>
  <SimPosition name="SimWandPos">
    <keyboardProxy>"WandKeyboardProxy" </keyboardProxy>
    <keyPairs>
        <KeyModPair name="SimWandPos-&gt;keyPairs-&gt;Forward">
          <modKey>"ALT" </modKey>
          <key>"MOUSE_NEGY" </key>
        </KeyModPair>

        <KeyModPair name="SimWandPos-&gt;keyPairs-&gt;Back">
          <modKey>"ALT" </modKey>
          <key>"MOUSE_POSY" </key>
        </KeyModPair>

        <KeyModPair name="SimWandPos-&gt;keyPairs-&gt;Left">
          <modKey>"CTRL" </modKey>
          <key>"MOUSE_NEGX" </key>
        </KeyModPair>

        <KeyModPair name="SimWandPos-&gt;keyPairs-&gt;Right">
          <modKey>"CTRL" </modKey>
          <key>"MOUSE_POSX" </key>
        </KeyModPair>

        <KeyModPair name="SimWandPos-&gt;keyPairs-&gt;Up">
          <modKey>"CTRL" </modKey>
          <key>"MOUSE_NEGY" </key>
        </KeyModPair>

        <KeyModPair name="SimWandPos-&gt;keyPairs-&gt;Down">
          <modKey>"CTRL" </modKey>
          <key>"MOUSE_POSY" </key>
        </KeyModPair>

        <KeyModPair name="SimWandPos-&gt;keyPairs-&gt;Rot Right">
          <modKey>"SHIFT" </modKey>
          <key>"MOUSE_NEGX" </key>
        </KeyModPair>

        <KeyModPair name="SimWandPos-&gt;keyPairs-&gt;Rot Left">
          <modKey>"SHIFT" </modKey>
          <key>"MOUSE_POSX" </key>
        </KeyModPair>

        <KeyModPair name="SimWandPos-&gt;keyPairs-&gt;Rot Up">
          <modKey>"SHIFT" </modKey>
          <key>"MOUSE_NEGY" </key>
        </KeyModPair>

        <KeyModPair name="SimWandPos-&gt;keyPairs-&gt;Rot Down">
          <modKey>"SHIFT" </modKey>
          <key>"MOUSE_POSY" </key>
        </KeyModPair>

        <KeyModPair name="SimWandPos-&gt;keyPairs-&gt;Rot CCW">
          <modKey>"NONE" </modKey>
          <key>"KEY_Z" </key>
        </KeyModPair>

        <KeyModPair name="SimWandPos-&gt;keyPairs-&gt;Rot CW">
          <modKey>"NONE" </modKey>
          <key>"KEY_X" </key>
        </KeyModPair>
</keyPairs>
    <initialPos>"1.0" "5.0" "0.0" </initialPos>
    <initialRot>"0.0" "0.0" "0.0" </initialRot>
    <dtrans>"0.01" </dtrans>
    <drot>"0.06" </drot>
    <transCoordSystem>"Global" </transCoordSystem>
    <rotCoordSystem>"Local" </rotCoordSystem>
  </SimPosition>
  <KeyboardProxy name="WandKeyboardProxy">
    <device>"Wand Keyboard" </device>
  </KeyboardProxy>
  <SimDigital name="SimWandButtons">
    <keyboardProxy>"WandKeyboardProxy" </keyboardProxy>
    <keyPairs>
