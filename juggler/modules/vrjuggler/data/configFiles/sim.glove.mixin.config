<ConfigChunkDB>
  <SimGloveGesture name="SimGloveGesture">
    <keyboardProxy>"GloveKeyProxy" </keyboardProxy>
    <keyPairs>
        <KeyModPair name="SimGloveGesture-&gt;keyPairs-&gt;keyPairs 0">
          <modKey>"NONE" </modKey>
          <key>"MOUSE_BUT1" </key>
        </KeyModPair>

        <KeyModPair name="SimGloveGesture-&gt;keyPairs-&gt;keyPairs 1">
          <modKey>"NONE" </modKey>
          <key>"MOUSE_BUT2" </key>
        </KeyModPair>

        <KeyModPair name="SimGloveGesture-&gt;keyPairs-&gt;keyPairs 2">
          <modKey>"NONE" </modKey>
          <key>"MOUSE_BUT3" </key>
        </KeyModPair>

        <KeyModPair name="SimGloveGesture-&gt;keyPairs-&gt;keyPairs 3">
          <modKey>"ALT" </modKey>
          <key>"MOUSE_BUT1" </key>
        </KeyModPair>
</keyPairs>
    <trainedFilename>"${VJ_BASE_DIR}/share/Data/gesture/simpleSimGestures.dat" </trainedFilename>
    <glovePos>"GlovePos Proxy" </glovePos>
  </SimGloveGesture>
  <KeyboardProxy name="GloveKeyProxy">
    <device>"Glove Keyboard" </device>
  </KeyboardProxy>
  <PositionProxy name="GlovePos Proxy">
    <device>"SimGlovePos" </device>
    <unit>"0" </unit>
    <etrans>"False" </etrans>
    <translate>"0.166" "0.0" "0.0" </translate>
    <rotate>"0.0" "0.0" "-90.0" </rotate>
    <useFilter>"False" </useFilter>
    <sigmoidParams>
        <sigmoid_linear_pos_filter name="GlovePos Proxy-&gt;sigmoidParams-&gt;sigmoidParams 0">
          <minDist>"0.0" </minDist>
          <maxDist>"0.0" </maxDist>
          <maxThreshold>"0.0" </maxThreshold>
        </sigmoid_linear_pos_filter>
</sigmoidParams>
  </PositionProxy>
  <SimPosition name="SimGlovePos">
    <keyboardProxy>"GloveKeyProxy" </keyboardProxy>
    <keyPairs>
        <KeyModPair name="SimGlovePos-&gt;keyPairs-&gt;Forward">
          <modKey>"ALT" </modKey>
          <key>"MOUSE_NEGY" </key>
        </KeyModPair>

        <KeyModPair name="SimGlovePos-&gt;keyPairs-&gt;Back">
          <modKey>"ALT" </modKey>
          <key>"MOUSE_POSY" </key>
        </KeyModPair>

        <KeyModPair name="SimGlovePos-&gt;keyPairs-&gt;Left">
          <modKey>"CTRL" </modKey>
          <key>"MOUSE_NEGX" </key>
        </KeyModPair>

        <KeyModPair name="SimGlovePos-&gt;keyPairs-&gt;Right">
          <modKey>"CTRL" </modKey>
          <key>"MOUSE_POSX" </key>
        </KeyModPair>

        <KeyModPair name="SimGlovePos-&gt;keyPairs-&gt;Up">
          <modKey>"CTRL" </modKey>
          <key>"MOUSE_NEGY" </key>
        </KeyModPair>

        <KeyModPair name="SimGlovePos-&gt;keyPairs-&gt;Down">
          <modKey>"CTRL" </modKey>
          <key>"MOUSE_POSY" </key>
        </KeyModPair>

        <KeyModPair name="SimGlovePos-&gt;keyPairs-&gt;Rot Right">
          <modKey>"SHIFT" </modKey>
          <key>"MOUSE_NEGX" </key>
        </KeyModPair>

        <KeyModPair name="SimGlovePos-&gt;keyPairs-&gt;Rot Left">
          <modKey>"SHIFT" </modKey>
          <key>"MOUSE_POSX" </key>
        </KeyModPair>

        <KeyModPair name="SimGlovePos-&gt;keyPairs-&gt;Rot Up">
          <modKey>"SHIFT" </modKey>
          <key>"MOUSE_NEGY" </key>
        </KeyModPair>

        <KeyModPair name="SimGlovePos-&gt;keyPairs-&gt;Rot Down">
          <modKey>"SHIFT" </modKey>
          <key>"MOUSE_POSY" </key>
        </KeyModPair>

        <KeyModPair name="SimGlovePos-&gt;keyPairs-&gt;Rot CCW">
          <modKey>"SHIFT" </modKey>
          <key>"MOUSE_BUT1" </key>
        </KeyModPair>

        <KeyModPair name="SimGlovePos-&gt;keyPairs-&gt;Rot CW">
          <modKey>"SHIFT" </modKey>
          <key>"MOUSE_BUT3" </key>
        </KeyModPair>
</keyPairs>
    <initialPos>"1.0" "5.0" "0.0" </initialPos>
    <initialRot>"0.0" "0.0" "0.0" </initialRot>
    <dtrans>"0.05" </dtrans>
    <drot>"0.5" </drot>
    <transCoordSystem>"Global" </transCoordSystem>
    <rotCoordSystem>"Local" </rotCoordSystem>
  </SimPosition>
  <Keyboard name="Glove Keyboard">
    <width>"200" </width>
    <height>"200" </height>
    <origin>"950" "85" </origin>
    <msens>"2.0" </msens>
    <display_number>"0" </display_number>
    <lock_key>"KEY_NONE" </lock_key>
    <start_locked>"False" </start_locked>
    <sleep_time>"0" </sleep_time>
  </Keyboard>
  <proxyAlias name="VJGlove">
    <aliasName>"VJGlove" </aliasName>
    <proxyPtr>"GloveProxy" </proxyPtr>
  </proxyAlias>
  <proxyAlias name="VJGesture">
    <aliasName>"VJGesture" </aliasName>
    <proxyPtr>"GestureProxy" </proxyPtr>
  </proxyAlias>
  <GloveProxy name="GloveProxy">
    <device>"SimGloveGesture" </device>
    <unit>"0" </unit>
    <visible>"True" </visible>
  </GloveProxy>
  <GestureProxy name="GestureProxy">
    <device>"SimGloveGesture" </device>
  </GestureProxy>
</ConfigChunkDB>
