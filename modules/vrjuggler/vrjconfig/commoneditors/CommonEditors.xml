<?xml version="1.0" encoding="UTF-8"?>
<beanlist xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:noNamespaceSchemaLocation="http://www.vrjuggler.org/tweek/xsd/1.1/beanlist.xsd">
   <service name="PositionalDeviceEditor">
      <file name="${TWEEK_BASE_DIR}/lib/vrjuggler/customeditors/CommonEditors.jar"
            class="org.vrjuggler.vrjconfig.commoneditors.PositionalDeviceEditor"/>
      <dependencies>
         <bean>ConfigBroker</bean>
         <jar path="${TWEEK_BASE_DIR}/bin/beans">jccl_editors.jar</jar>
         <jar path="${TWEEK_BASE_DIR}/bin">ui.jar</jar>
         <jar path="${TWEEK_BASE_DIR}/bin">jogl-demos-data.jar</jar>
         <jar path="${TWEEK_BASE_DIR}/bin">jogl-demos.jar</jar>
         <jar path="${TWEEK_BASE_DIR}/bin">jogl-demos-util.jar</jar>
         <jar path="${TWEEK_BASE_DIR}/bin">jogl.jar</jar>
      </dependencies>
   </service>
</beanlist>
