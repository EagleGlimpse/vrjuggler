<ConfigChunkDB>
  <AnaProxy name="AnaProxy 0">
    <device>"SimAnalogDevice" </device>
    <unit>"0" </unit>
  </AnaProxy>
  <AnaProxy name="AnaProxy 1">
    <device>"SimAnalogDevice" </device>
    <unit>"1" </unit>
  </AnaProxy>
  <SimAnalog name="SimAnalogDevice">
    <keyboardProxy>"WandKeyboardProxy" </keyboardProxy>
    <keyPairsInc>
        <KeyModPair name="SimAnalogDevice-&gt;keyPairsInc-&gt;Increment Keypress 0">
          <modKey>"NONE" </modKey>
          <key>"KEY_Q" </key>
        </KeyModPair>

        <KeyModPair name="SimAnalogDevice-&gt;keyPairsInc-&gt;Increment Keypress 1">
          <modKey>"NONE" </modKey>
          <key>"KEY_W" </key>
        </KeyModPair>

        <KeyModPair name="SimAnalogDevice-&gt;keyPairsInc-&gt;Increment Keypress 2">
          <modKey>"NONE" </modKey>
          <key>"KEY_E" </key>
        </KeyModPair>

        <KeyModPair name="SimAnalogDevice-&gt;keyPairsInc-&gt;Increment Keypress 3">
          <modKey>"NONE" </modKey>
          <key>"KEY_R" </key>
        </KeyModPair>
</keyPairsInc>
    <keyPairsDec>
        <KeyModPair name="SimAnalogDevice-&gt;keyPairsDec-&gt;Decrement Keypress 0">
          <modKey>"NONE" </modKey>
          <key>"KEY_A" </key>
        </KeyModPair>

        <KeyModPair name="SimAnalogDevice-&gt;keyPairsDec-&gt;Decrement Keypress 1">
          <modKey>"NONE" </modKey>
          <key>"KEY_S" </key>
        </KeyModPair>

        <KeyModPair name="SimAnalogDevice-&gt;keyPairsDec-&gt;Decrement Keypress 2">
          <modKey>"NONE" </modKey>
          <key>"KEY_D" </key>
        </KeyModPair>

        <KeyModPair name="SimAnalogDevice-&gt;keyPairsDec-&gt;Decrement Keypress 3">
          <modKey>"NONE" </modKey>
          <key>"KEY_F" </key>
        </KeyModPair>
</keyPairsDec>
    <anastep>"1.0" </anastep>
    <min>"0.0" </min>
    <max>"255.0" </max>
  </SimAnalog>
  <proxyAlias name="VJAnalog0">
    <aliasName>"VJAnalog0" </aliasName>
    <proxyPtr>"AnaProxy 0" </proxyPtr>
  </proxyAlias>
  <proxyAlias name="VJAnalog1">
    <aliasName>"VJAnalog1" </aliasName>
    <proxyPtr>"AnaProxy 1" </proxyPtr>
  </proxyAlias>
</ConfigChunkDB>
