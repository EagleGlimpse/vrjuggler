<ConfigChunkDB>
  <SimAnalog name="AnalogSimulator0">
    <keyboardProxy>"SimKeyboard Ana Proxy" </keyboardProxy>
    <keyPairsInc>
        <KeyModPair name="AnalogSimulator0-&gt;keyPairsInc-&gt;Increment Keypress 0">
          <modKey>"NONE" </modKey>
          <key>"KEY_Q" </key>
        </KeyModPair>

        <KeyModPair name="AnalogSimulator0-&gt;keyPairsInc-&gt;Increment Keypress 1">
          <modKey>"NONE" </modKey>
          <key>"KEY_W" </key>
        </KeyModPair>
</keyPairsInc>
    <keyPairsDec>
        <KeyModPair name="AnalogSimulator0-&gt;keyPairsDec-&gt;Decrement Keypress 0">
          <modKey>"NONE" </modKey>
          <key>"KEY_A" </key>
        </KeyModPair>

        <KeyModPair name="AnalogSimulator0-&gt;keyPairsDec-&gt;Decrement Keypress 1">
          <modKey>"NONE" </modKey>
          <key>"KEY_S" </key>
        </KeyModPair>
</keyPairsDec>
    <anastep>"1.0" </anastep>
    <min>"0.0" </min>
    <max>"255.0" </max>
  </SimAnalog>
  <Keyboard name="Sim Analog Input">
    <width>"300" </width>
    <height>"100" </height>
    <origin>"870" "100" </origin>
    <msens>"1.0" </msens>
    <display_number>"0" </display_number>
    <lock_key>"KEY_NONE" </lock_key>
    <start_locked>"False" </start_locked>
    <sleep_time>"0" </sleep_time>
  </Keyboard>
  <KeyboardProxy name="SimKeyboard Ana Proxy">
    <device>"Sim Analog Input" </device>
  </KeyboardProxy>
  <AnaProxy name="AnalogProxy0">
    <device>"AnalogSimulator0" </device>
    <unit>"0" </unit>
  </AnaProxy>
  <AnaProxy name="AnalogProxy1">
    <device>"AnalogSimulator0" </device>
    <unit>"1" </unit>
  </AnaProxy>
  <proxyAlias name="VJAnalog0">
    <aliasName>"VJAnalog0" </aliasName>
    <proxyPtr>"AnalogProxy0" </proxyPtr>
  </proxyAlias>
  <proxyAlias name="VJAnalog1">
    <aliasName>"VJAnalog1" </aliasName>
    <proxyPtr>"AnalogProxy1" </proxyPtr>
  </proxyAlias>
</ConfigChunkDB>
